repeat task.wait() until game:IsLoaded()
repeat task.wait() until game:GetService("Players").LocalPlayer

task.wait(5)

local found = false
local Notify = loadstring(game:HttpGet("https://raw.githubusercontent.com/Achitsak/scripts/main/ui/v1.lua"))()

local GameList = {
    '4372130',
    '5348890',
    '7381705',
    '10611639',
    '33820338'
}

print(`Service Version 1.2.0`)
 
_G.notify({
    text = "Service Loaded Successfully", 
    type = "INFO", 
    duration = 5
})

task.wait(math.random(2,5))

_G.notify({
    text = "Auth Checking [Server]", 
    type = "WARNING", 
    duration = 2
})

task.wait(math.random(2,5))

if not _G.Key or not _G.Discord then
    while true do
        _G.notify({
            text = "Your Are Not Whitelist", 
            type = "WARNING", 
            duration = 3
        })
        wait(5)
    end
else
    _G.notify({
        text = "Auth Checking [Client]", 
        type = "WARNING", 
        duration = 3
    })
end

for _, id in ipairs(GameList) do
    if id == game.CreatorId then
        found = true
        break
    end
end

if not found then
    while true do
        _G.notify({
            text = "Game Not Supported", 
            type = "ERROR", 
            duration = 5
        })
    task.wait(4)
    end
end

local url = ("https://raw.githubusercontent.com/Achitsak/scripts/main/library/description/%s.lua"):format(game.CreatorId)
pcall(function() loadstring(game:HttpGet(url))() end)

loadstring(game:HttpGet("https://raw.githubusercontent.com/Achitsak/scripts/main/backend"))()

_G.notify({
    text = "All Services Is Loaded ðŸ˜Ž", 
    type = "INFO", 
    duration = 5
})